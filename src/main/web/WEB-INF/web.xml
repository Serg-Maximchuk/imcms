    <filter>
        <description>Imcms Maintenance Filter</description>
        <filter-name>MaintenanceFilter</filter-name>
        <filter-class>com.imcode.imcms.maintenance.MaintenanceFilter</filter-class>
    </filter>
    
    <filter>
        <description>Imcms Filter</description>
        <filter-name>ImcmsFilter</filter-name>
        <filter-class>com.imcode.imcms.servlet.ImcmsFilter</filter-class>
    </filter>

    <filter>
        <description>Restrict api samples to superadmin.</description>
        <filter-name>SuperAdminFilter</filter-name>
        <filter-class>com.imcode.imcms.servlet.SuperAdminFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>MaintenanceFilter</filter-name>
        <url-pattern>/admin/maintenance/*</url-pattern>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>

    <filter-mapping>
        <filter-name>ImcmsFilter</filter-name>
        <url-pattern>/*</url-pattern>
        <!--<dispatcher>REQUEST</dispatcher>-->
    </filter-mapping>

    <!--
    <filter-mapping>
