    <table name="text_docs">
        <column name="meta_id" primaryKey="true" required="true" type="INTEGER" />
        <column name="template_name" required="true" type="VARCHAR" size="255" />
        <column default="1" name="group_id" required="true" type="INTEGER" />
        <column name="default_template_1" type="VARCHAR" size="255" />
        <column name="default_template_2" type="VARCHAR" size="255" />
        <column name="default_template" type="VARCHAR" size="255"/>
        <foreign-key foreignTable="meta">
            <reference foreign="meta_id" local="meta_id" />
        </foreign-key>
    </table>
