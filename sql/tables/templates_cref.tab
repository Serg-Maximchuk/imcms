	<table name="templates_cref">
        <column name="group_id" primaryKey="true" required="true" type="INTEGER" autoIncrement="false" />
        <column name="template_id" primaryKey="true" required="true" type="INTEGER" autoIncrement="false" />
        <foreign-key foreignTable="templategroups">
            <reference foreign="group_id" local="group_id" />
        </foreign-key>
        <foreign-key foreignTable="templates">
            <reference foreign="template_id" local="template_id" />
        </foreign-key>
    </table>
