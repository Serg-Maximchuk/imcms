	<table name="B_section">
        <column autoIncrement="true" name="section_id" primaryKey="true" required="true" type="INTEGER" />
        <column name="section_name" size="255" type="VARCHAR" required="false" />
        <column name="archive_time" required="true" type="TIMESTAMP" />
        <column name="archive_mode" required="true" size="1" type="CHAR" />
        <column name="days_to_show" required="true" type="INTEGER" />
        <column default="20" name="discs_to_show" required="true" type="INTEGER" />
    </table>
