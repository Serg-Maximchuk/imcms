	<table name="A_conf_forum">
        <column name="conf_id" primaryKey="true" required="true" type="INTEGER" />
        <column name="forum_id" primaryKey="true" required="true" type="INTEGER" />
        <column name="sort_number" type="INTEGER" required="false" />
        <foreign-key foreignTable="A_conference">
            <reference foreign="meta_id" local="conf_id" />
        </foreign-key>
        <foreign-key foreignTable="A_forum">
            <reference foreign="forum_id" local="forum_id" />
        </foreign-key>
    </table>
