CREATE TABLE [dbo].[classification] (
	[class_id] [int] IDENTITY (1, 1) NOT NULL ,
	[code] [varchar] (30) NOT NULL ,
	CONSTRAINT [PK_classification] PRIMARY KEY  NONCLUSTERED 
	(
		[class_id]
	)  ON [PRIMARY] 
) ON [PRIMARY]
GO


