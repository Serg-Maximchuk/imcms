    <table name="url_docs">
        <column name="meta_id" primaryKey="true" required="true" type="INTEGER" />
        <column name="frame_name" required="false" size="80" type="VARCHAR" />
        <column name="target" required="false" size="15" type="VARCHAR" />
        <column name="url_ref" required="false" size="255" type="VARCHAR" />
        <column name="url_txt" required="false" size="255" type="VARCHAR" />
        <column name="lang_prefix" primaryKey="true" required="false" size="3" type="VARCHAR" />
        <foreign-key foreignTable="meta">
            <reference foreign="meta_id" local="meta_id" />
        </foreign-key>
    </table>
